{
    "name": "telenok/core",
    "description": "Telenok CMS Core engine",
    "keywords": ["telenok", "core", "laravel"],
    "homepage": "http://telenok.com",
    "license": "commercial",
    "support": {
        "email": "support@telenok.com",
        "issues": "https://github.com/telenok/core/issues"
    },
    "authors": [
        {
            "name": "Kiselevich Konstantin",
            "email": "support@telenok.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "ext-bcmath": "*",
        "laravel/framework": "~5.1",
        "illuminate/html": "5.*",
        "composer/composer": "1.0.0-alpha10",
        "imagine/imagine": "~0.5"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-0": {
            "Telenok\\Core": "src/"
        }
    },
    "scripts": {
        "post-update-cmd": [
            "php artisan vendor:publish --tag=\"public\" --provider=\"Telenok\\Core\\Providers\\CoreServiceProvider\" --force",
            "php artisan migrate --force --path=\"vendor/telenok/core/src/migrations\""
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
